  "license": "GPL-3.0",
  "devDependencies": {
    "@types/i18next": "^8.4.0",
    "@types/react": "16.0.34",
    "@types/react-bootstrap": "^0.0.37",
    "@types/react-redux": "4.4.35",
    "vortex-api": "Nexus-Mods/vortex-api",
    "react-bootstrap": "^0.30.6",
    "webpack": "^1.13.2"
  }
}
